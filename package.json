{
  "name": "strucdoceditor",
  "version": "1.0.0",
  "description": "A structure documentation editor in web with TS.",
  "private": false,
  "main": "./dist/main.bundle.js",
  "types": "./dist/types/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha --reporter spec",
    "tsbuild": "tsc",
    "build": "webpack --config webpack.config.js --mode=production --node-env=production",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build:dev": "webpack --config webpack.config.js --mode=development",
    "build:prod": "webpack --config webpack.config.js --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dereking/strucdoceditor.git"
  },
  "keywords": [
    "structure documentation",
    "editor"
  ],
  "author": "Dereking",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dereking/strucdoceditor/issues"
  },
  "homepage": "https://github.com/Dereking/strucdoceditor#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@webpack-cli/generators": "^2.4.2",
    "chai": "^4.3.6",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mocha": "^10.0.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
